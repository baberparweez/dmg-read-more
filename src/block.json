{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/dmg-read-more",
	"version": "0.1.0",
	"title": "DMG Read More",
	"category": "widgets",
	"icon": "smiley",
	"description": "A WordPress plugin to provide users with a custom Gutenberg block that allows searching for posts.",
	"attributes": {
		"selectedPostId": {
			"type": "number"
		},
		"selectedPostTitle": {
			"type": "string"
		},
		"selectedPostPermalink": {
			"type": "string"
		}
	},
	"supports": {
		"html": false
	},
	"textdomain": "dmg-read-more",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
